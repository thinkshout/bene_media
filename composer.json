{
    "name": "thinkshout/bene_media",
    "description": "Media related functionality and configuration for the Bene Distribution.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "ThinkShout",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/focal_point": "^1.0@beta",
        "drupal/config_rewrite": "1.1",
        "drupal/crop": "^1.5",
        "drupal/core": "^8.8.3",
        "drupal/ctools": "^3.0",
        "drupal/entity_browser": "^1.8",
        "drupal/entity_embed": "^1.3",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/picture_background_formatter": "1.x-dev",
        "drupal/video_embed_field": "^2.0@alpha",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/media_entity_actions": "^1.0@alpha"
    },
    "extra": {
        "patches": {
            "drupal/embed": {
                "2824110 - Allow site-builders to use a non-managed file as button icon":
                "https://www.drupal.org/files/issues/2019-12-30/embed-allow-non-managed-button-icon-files--2824110-39_0.patch"
            }
        }
    }
}
