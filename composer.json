{
    "name": "thinkshout/bene_media",
    "description": "Media related functionality and configuration for the Bene Distribution.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "ThinkShout",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/focal_point": "^1.0@beta",
        "drupal/crop": "^1.3",
        "drupal/entity_browser": "^1.8",
        "drupal/entity_embed": "^1.3",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/media_entity_document": "^1.1",
        "drupal/media_entity_image": "^1.2",
        "drupal/media_entity_instagram": "^1.0",
        "drupal/media_entity_twitter": "^1.0",
        "drupal/picture_background_formatter": "1.x-dev",
        "drupal/video_embed_media": "^1.5",
        "drupal/views_infinite_scroll": "^1.5"
    },
    "patches": {
        "drupal/entity_embed": {
            "2832504 - Send the CKEditor instance ID to the embed.preview route":
            "https://www.drupal.org/files/issues/2019-12-16/embed-allow-non-managed-button-icon-files--2824110-35.patch"
        },
        "drupal/embed": {
            "2824110 - Allow site-builders to use a non-managed file as button icon":
            "https://www.drupal.org/files/issues/2019-12-16/embed-allow-non-managed-button-icon-files--2824110-35.patch"
        }
    }
}
