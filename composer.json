{
    "name": "thinkshout/bene_media",
    "description": "Media related functionality and configuration for the Bene Distribution.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "ThinkShout",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/focal_point": "^1.4",
        "drupal/config_rewrite": "^1.1",
        "drupal/crop": "^2.0",
        "drupal/ctools": "^3.0",
        "drupal/entity_browser": "^1.10",
        "drupal/entity_embed": "^1.1",
        "drupal/inline_entity_form": "^1.0-rc6",
        "drupal/picture_background_formatter": "^1.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/media_entity": "^2.0-beta7",
        "drupal/media_entity_actions": "^1.0@alpha",
        "drupal/media_entity_document": "^1.1",
        "drupal/media_entity_image": "^1.3",
        "drupal/media_entity_instagram": "^2.1",
        "drupal/media_entity_twitter": "^2.4"
    },
    "patches": {
        "drupal/entity_embed": {
            "2824110 - Allow site-builders to use a non-managed file as button icon":
            "https://www.drupal.org/files/issues/2019-12-30/embed-allow-non-managed-button-icon-files--2824110-39_0.patch"
        }
    }
}
